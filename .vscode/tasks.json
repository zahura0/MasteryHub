{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "dev:frontend",
			"type": "shell",
			"command": "npm",
			"args": [
				"run",
				"-s",
				"-w",
				"frontend",
				"dev"
			],
			"isBackground": true,
			"problemMatcher": [
				"$eslint-stylish"
			],
			"group": "build"
		},
		{
			"label": "lint:frontend",
			"type": "shell",
			"command": "npm",
			"args": [
				"run",
				"-s",
				"-w",
				"frontend",
				"lint"
			],
			"isBackground": false,
			"problemMatcher": [
				"$eslint-stylish"
			],
			"group": "build"
		}
	]
}